-19","pageNames":["page-home"],"classList":["website-min-width"],"type":"section-element","isSectionContent":false,"title":"","alt":null,"children":["silex-id-1483131242514-20"],"link":null,"enableEdit":false,"enableDrag":true,"enableDrop":true,"enableResize":{"top":false,"bottom":false,"left":false,"right":false},"selected":false,"useMinHeight":true,"visibility":{"desktop":true,"mobile":true},"style":{"desktop":{"position":"static","margin-top":"-1px"},"mobile":{"top":"2205px","left":"0px"}},"data":{"component":null},"innerHtml":"","tagName":"SECTION"},{"id":"silex-id-1483131242514-20","pageNames":[],"classList":["website-width"],"type":"container-element","isSectionContent":true,"title":"","alt":null,"children":["silex-id-1483131242515-22","silex-id-1483131242515-23"],"link":null,"enableEdit":false,"enableDrag":false,"enableDrop":true,"enableResize":{"top":true,"bottom":true,"left":true,"right":true},"selected":false,"useMinHeight":true,"visibility":{"desktop":true,"mobile":true},"style":{"desktop":{"background-color":"transparent","height":"355px","position":"relative","margin-left":"auto","margin-right":"auto"},"mobile":{"height":"456.433349609375px"}},"data":{"component":null},"innerHtml":"","tagName":"DIV"},{"id":"silex-id-1483131242515-22","pageNames":[],"classList":["full-width","grey-text"],"type":"text-element","isSectionContent":false,"title":"","alt":null,"children":[],"link":null,"enableEdit":true,"enableDrag":true,"enableDrop":false,"enableResize":{"top":true,"bottom":true,"left":true,"right":true},"selected":false,"useMinHeight":true,"visibility":{"desktop":true,"mobile":false},"style":{"desktop":{"width":"978px","top":"22px","left":"1px","height":"26px","position":"absolute"},"mobile":{}},"data":{"component":null},"innerHtml":"\n                    /////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////\n                ","tagName":"DIV"},{"i